                        <span class="user-img"><img src="/Images/review3.png" alt="">
                        <span class="status online"></span></span>
                        <span><%=user.name %> </span>
                    </a>
                    <div class="dropdown-menu">
                        <a class="dropdown-item" href="/employeeprofile">My Profile</a>
                        <a class="dropdown-item" href="#">Settings</a>
                        <a class="dropdown-item" href="/login">Logout</a>
                    </div>
                </li>
            </ul>
            <!-- /Header Menu -->
            
        </div>
        <!-- /Header -->

        <div class="navigation">
            <ul>
                <li class="active-btn">
                    <a href="/dashboard">
